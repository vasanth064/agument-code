{
  "name": "agument-code",
  "version": "1.0.0",
  "scripts": {
    "build": "if-env TYPE=server && (cd server && yarn) || (cd client && yarn)",
    "client": "cd client && yarn dev",
    "server": "cd server && yarn dev",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": "git@github.com:vasanth064/agument-code.git",
  "author": "vasanth064 <vasanthvdev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.0.1",
    "if-env": "^1.0.4"
  }
}
